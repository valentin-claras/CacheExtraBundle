{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
        <img width="28" height="28" alt="Ladybug" style="vertical-align: middle; margin-right: 5px;" src="data:image/png;base64,"/>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Cache HIT</b>
            <span>{{ collector.hits }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Cache Miss</b>
            <span>{{ collector.miss }}</span>
        </div>
    {% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': true } %}

    <style type="text/css">
        .cachedBlock { position: relative; }

        .cachedBlock .cb-overlay {
            position: absolute; width: 100%; height: 100%; top: 0; left: 0;
            z-index: 10000; margin: -2px 0 0 -2px; pointer-events: none;
        }
        .cachedBlock:hover .cb-overlay { opacity: 0.1; }

        .cachedBlock .cb-overlay.cb-hit { border: 2px dashed green; }
        .cachedBlock .cb-overlay.cb-miss { border: 2px dashed red; }

        .cachedBlock .cb-overlay > div {
            display: block; line-height: 1; width: 100%;
            margin: -2px 0 0 -2px; padding: 2px;
            font-weight: bold; color: #FFF;
            font-size: 12px;
        }

        .cachedBlock .cb-overlay > div > span { display: block; }

        .cachedBlock .cb-overlay.cb-hit > div { background-color: green; }
        .cachedBlock .cb-overlay.cb-miss > div { background-color: red; }
    </style>
{% endblock %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon">{% spaceless %}
    <img src="data:image/png;base64," style="padding-top:5px;" alt="Dependency Injection Container" />
    {% endspaceless %}</span>
    <strong>Action Cache</strong>
    <span class="count">
        <span>{{ collector.hits }}</span>
    </span>
</span>
{% endblock %}

{% block panel %}
    <h2>Cached controllers</h2>
    <ul>
        <li>
            <table class="routing inline">
                <tr>
                    <th>Controller</th>
                    <th>Cache status</th>
                </tr>
            {% for controllerName, cached in collector.blocks %}
                <tr>
                    <td>{{ controllerName }}</td>
                    <td>{{ cached ? "CACHE HIT" : "CACHE MISS" }}</td>
                </tr>
            {% endfor %}
        </table>
    </li>
</ul>

{% endblock %}
